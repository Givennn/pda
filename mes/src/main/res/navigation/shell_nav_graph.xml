    <fragment
        android:id="@+id/equipmentProduceHistoryListFragment"
        android:name="com.panda.pda.mes.operation.ems.equipment_workorder.EquipmentProduceHistoryListFragment"
        android:label="EquipmentProduceHistoryListFragment" />
    <!--工单添加-->
    <fragment
        android:id="@+id/equipmentInfoWorkOrderAddFragment"
        android:name="com.panda.pda.mes.operation.ems.equipment_workorder.EquipmentInfoWorkOrderAddFragment"
        android:label="EquipmentInfoWorkOrderAddFragment" />
    <!--去分配-->
    <fragment
        android:id="@+id/equipmentWorkOrderWaitFenpeiFragment"
        android:name="com.panda.pda.mes.operation.ems.equipment_workorder.maintenance.EquipmentWorkOrderWaitFenpeiFragment"
        android:label="EquipmentWorkOrderWaitFenpeiFragment" />
    <!--去完工-->
    <fragment
        android:id="@+id/equipmentWorkOrderWaitCompleteFragment"
        android:name="com.panda.pda.mes.operation.ems.equipment_workorder.maintenance.EquipmentWorkOrderWaitCompleteFragment"
        android:label="EquipmentWorkOrderWaitCompleteFragment" />
    <!--维保完成去确认-->
    <fragment
        android:id="@+id/equipmentInfoWorkOrderMaintenanceCompleteFragment"
        android:name="com.panda.pda.mes.operation.ems.equipment_workorder.maintenance.EquipmentInfoWorkOrderMaintenanceCompleteFragment"
        android:label="EquipmentInfoWorkOrderMaintenanceCompleteFragment" />
    <!--人员选择-->
    <fragment
        android:id="@+id/equipmentPersonChooseListFragment"
        android:name="com.panda.pda.mes.operation.ems.equipment_workorder.maintenance.EquipmentPersonChooseListFragment"
        android:label="EquipmentPersonChooseListFragment" />
    <!--出库时厂商选择-->
    <fragment
        android:id="@+id/equipmentSupplierChooseListFragment"
        android:name="com.panda.pda.mes.operation.ems.equipment_workorder.store.EquipmentSupplierChooseListFragment"
        android:label="EquipmentSupplierChooseListFragment" />
