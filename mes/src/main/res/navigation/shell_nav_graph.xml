    <!--ems任务-->
    <fragment
        android:id="@+id/equipmentTaskFragment"
        android:name="com.panda.pda.mes.operation.ems.equipment_task.EquipmentWorkOrderTaskFragment"
        android:label="fragment_equipment_task">
    </fragment>
    <!--ems维保页面-->
    <fragment
        android:id="@+id/equipmentWorkOrderFragment"
        android:name="com.panda.pda.mes.operation.ems.equipment_workorder.EquipmentWorkOrderListFragment"
        android:label="fragment_equipment_task">
        <!--工单填报-->
        <action
            android:id="@+id/action_equipmentWorkOrderFragment_to_equipmentInfoWorkOrderAddFragment"
            app:destination="@id/equipmentInfoWorkOrderAddFragment" />
        <!--去分配-->
        <action
            android:id="@+id/action_equipmentWorkOrderFragment_to_EquipmentWorkOrderWaitFenpeiFragment"
            app:destination="@id/equipmentWorkOrderWaitFenpeiFragment" />
        <!--待完工-->
        <action
            android:id="@+id/action_equipmentWorkOrderFragment_to_equipmentWorkOrderWaitCompleteFragment"
            app:destination="@id/equipmentWorkOrderWaitCompleteFragment" />
        <!--维保完成去确认-->
        <action
            android:id="@+id/action_equipmentWorkOrderFragment_to_equipmentInfoWorkOrderWaitInStoreFragment"
            app:destination="@id/equipmentInfoWorkOrderWaitInStoreFragment" />
        <!--维保完成去确认-->
        <action
            android:id="@+id/action_equipmentWorkOrderFragment_to_equipmentInfoWorkOrderMaintenanceCompleteFragment"
            app:destination="@id/equipmentInfoWorkOrderMaintenanceCompleteFragment" />

    </fragment>
