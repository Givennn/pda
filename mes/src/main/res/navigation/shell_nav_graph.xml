    <fragment
        android:id="@+id/equipmentProductChooseListFragment"
        android:name="com.panda.pda.mes.operation.ems.equipment_workorder.store.EquipmentProductChooseListFragment"
        android:label="EquipmentProductChooseListFragment" />
    <!--提交入库-->
    <fragment
        android:id="@+id/equipmentInfoWorkOrderWaitInStoreFragment"
        android:name="com.panda.pda.mes.operation.ems.equipment_workorder.store.EquipmentInfoWorkOrderWaitInStoreFragment"
        android:label="EquipmentInfoWorkOrderWaitInStoreFragment" />
    <!--确认出库-->
    <fragment
        android:id="@+id/equipmentInfoWorkOrderConfirmOutStoreFragment"
        android:name="com.panda.pda.mes.operation.ems.equipment_workorder.store.EquipmentInfoWorkOrderConfirmOutStoreFragment"
        android:label="EquipmentInfoWorkOrderConfirmOutStoreFragment" />
    <!--确认入库-->
    <fragment
        android:id="@+id/equipmentInfoWorkOrderInStoreCompleteFragment"
        android:name="com.panda.pda.mes.operation.ems.equipment_workorder.store.EquipmentInfoWorkOrderInStoreCompleteFragment"
        android:label="EquipmentInfoWorkOrderInStoreCompleteFragment" />
