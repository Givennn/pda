    <!--操作记录图文详情-->
    <fragment
        android:id="@+id/equipmentOperationDetailFragment"
        android:name="com.panda.pda.mes.operation.ems.equipment_workorder.EquipmentOperationDetailFragment"
        android:label="EquipmentOperationDetailFragment" />

