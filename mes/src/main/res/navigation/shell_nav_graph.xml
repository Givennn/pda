        android:name="com.panda.pda.mes.operation.ems.equipment_workorder.store.EquipmentDeviceChooseListFragment"
        android:label="EquipmentDeviceChooseListFragment" />
    <!--工单详情-->
    <fragment
        android:id="@+id/equipmentWorkOrderDetailFragment"
        android:name="com.panda.pda.mes.operation.ems.equipment_workorder.EquipmentWorkOrderDetailFragment"
        android:label="EquipmentWorkOrderDetailFragment" />
